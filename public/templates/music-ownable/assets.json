{
  "index.html": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <style>\n      #container {\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        justify-content: center;\n        height: 100vh;\n        background-size: cover;\n      }\n\n      #image-container {\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        border-radius: 20px;\n        padding-top: 20px;\n        position: relative;\n        width: 280px;\n        height: 280px;\n        perspective: 1000px;\n      }\n\n      #image-container img {\n        position: absolute;\n        width: 90%;\n        height: 90%;\n        object-fit: cover;\n        backface-visibility: hidden;\n        transition: transform 0.5s ease;\n        cursor: pointer;\n        background-color: #717171;\n      }\n\n      #image-container img.flipped {\n        transform: rotateY(180deg);\n      }\n\n      #image-container img {\n        transition: transform 2s ease;\n      }\n\n      .dot {\n        height: 10px;\n        width: 10px;\n        background-color: #bbb;\n        border-radius: 50%;\n        display: inline-block;\n        margin: 0 5px;\n        cursor: pointer;\n      }\n\n      .active {\n        background-color: #717171;\n      }\n\n      #image,\n      #second-image {\n        border-radius: 30px;\n      }\n\n      #audioPlayer {\n        display: flex;\n        align-items: center;\n        width: 250px;\n        background-color: rgba(242, 242, 242, 0.9);\n        border-radius: 20px;\n        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        margin-top: 20px;\n      }\n\n      #audioPlayer button {\n        margin-right: 5px;\n        background-color: transparent;\n        border: none;\n        color: grey;\n        padding: 3px;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        font-size: 15px;\n        transition-duration: 0.4s;\n        cursor: pointer;\n        border-radius: 50%;\n        width: 30px;\n        height: 30px;\n      }\n\n      #audioPlayer button:hover {\n        background-color: transparent;\n      }\n\n      #audioPlayer span {\n        margin: 0 10px;\n      }\n\n      #audioPlayer input[type=\"range\"] {\n        flex-grow: 1;\n        margin: 0 10px;\n      }\n\n      #audioPlayer input[type=\"range\"]::-webkit-slider-thumb {\n        -webkit-appearance: none;\n        appearance: none;\n        width: 5px;\n        height: 5px;\n        background: #000000;\n        cursor: pointer;\n        border-radius: 50%;\n      }\n\n      #audioPlayer input[type=\"range\"]::-moz-range-thumb {\n        width: 5px;\n        height: 5px;\n        background: #000000;\n        cursor: pointer;\n        border-radius: 50%;\n      }\n\n      #timeDisplay {\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n        width: 60px;\n      }\n\n      #timeDisplay span {\n        font-size: 14px;\n        margin: 0;\n      }\n\n      @media (max-width: 220px) {\n        body,\n        html {\n          height: 100%;\n          margin: 0;\n          padding: 0;\n          overflow: hidden;\n        }\n        #container {\n          width: 100%;\n          height: 100%;\n        }\n        #image-container {\n          width: 220px;\n          height: 220px;\n          padding-top: 0;\n          padding-bottom: 0;\n          border-radius: 0px;\n        }\n        #image-container img {\n          width: 100%;\n          height: 100%;\n        }\n        .dot {\n          display: none;\n        }\n        #audioPlayer {\n          display: none;\n        }\n      }\n\n      @media (min-width: 220px) and (max-width: 450px) {\n        body,\n        html {\n          height: 100%;\n          margin: 0;\n          padding: 0;\n          overflow: hidden;\n        }\n        #container {\n          width: 100%;\n          height: 100%;\n        }\n        #image-container {\n          width: 320px;\n          height: 320px;\n          padding-top: 0;\n          padding-bottom: 0;\n          border-radius: 30px;\n        }\n      }\n    </style>\n    <meta charset=\"UTF-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n    <title>PLACEHOLDER2_TITLE</title>\n  </head>\n  <body>\n    <div id=\"container\">\n      <div id=\"image-container\">\n        <img\n          id=\"image\"\n          src=\"PLACEHOLDER2_COVER\"\n          alt=\"Cover Art\"\n          onclick=\"flipImage(1)\"\n        />\n        <img\n          id=\"second-image\"\n          src=\"PLACEHOLDER2_BACKGROUND\"\n          alt=\"Backdrop\"\n          class=\"flipped\"\n          onclick=\"flipImage(0)\"\n        />\n      </div>\n      <div>\n        <span class=\"dot active\" onclick=\"flipImage(0)\"></span>\n        <span class=\"dot\" onclick=\"flipImage(1)\"></span>\n      </div>\n      <div id=\"audioPlayer\">\n        <audio id=\"myAudio\" preload=\"metadata\">\n          <source src=\"PLACEHOLDER2_AUDIO\" type=\"audio/mpeg\" />\n          Your browser does not support the audio element.\n        </audio>\n        <button id=\"playButton\">&#x25B6;</button>\n        <span id=\"timeDisplay\">\n          <span id=\"currentTime\">0:00</span> / <span id=\"totalTime\">0:00</span>\n        </span>\n        <input id=\"progressBar\" type=\"range\" min=\"0\" value=\"0\" step=\"1\" />\n      </div>\n    </div>\n    <script>\n      function flipImage(index) {\n        if (window.innerWidth > 200) {\n          var firstImage = document.getElementById(\"image\");\n          var secondImage = document.getElementById(\"second-image\");\n          var dots = document.getElementsByClassName(\"dot\");\n\n          if (index === 0) {\n            firstImage.classList.remove(\"flipped\");\n            secondImage.classList.add(\"flipped\");\n            dots[0].classList.add(\"active\");\n            dots[1].classList.remove(\"active\");\n          } else if (index === 1) {\n            firstImage.classList.add(\"flipped\");\n            secondImage.classList.remove(\"flipped\");\n            dots[0].classList.remove(\"active\");\n            dots[1].classList.add(\"active\");\n          }\n        }\n      }\n\n      var audio = document.getElementById(\"myAudio\");\n      var playButton = document.getElementById(\"playButton\");\n      var progressBar = document.getElementById(\"progressBar\");\n      var currentTime = document.getElementById(\"currentTime\");\n      var totalTime = document.getElementById(\"totalTime\");\n\n      audio.addEventListener(\"loadedmetadata\", function () {\n        progressBar.max = audio.duration;\n        var minutes = Math.floor(audio.duration / 60);\n        var seconds = Math.floor(audio.duration % 60);\n        totalTime.textContent =\n          minutes + \":\" + (seconds < 10 ? \"0\" : \"\") + seconds;\n      });\n\n      audio.addEventListener(\"timeupdate\", function () {\n        progressBar.value = audio.currentTime;\n        var minutes = Math.floor(audio.currentTime / 60);\n        var seconds = Math.floor(audio.currentTime % 60);\n        currentTime.textContent =\n          minutes + \":\" + (seconds < 10 ? \"0\" : \"\") + seconds;\n      });\n\n      progressBar.addEventListener(\"input\", function () {\n        audio.currentTime = progressBar.value;\n      });\n\n      playButton.addEventListener(\"click\", function () {\n        if (audio.paused) {\n          audio.play();\n          playButton.innerHTML = \"&#x23F8;\";\n        } else {\n          audio.pause();\n          playButton.innerHTML = \"&#x25B6;\";\n        }\n      });\n    </script>\n  </body>\n</html>\n"
}